<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="net.dassi.services.api.Service" table="services" catalog="dassi_net" optimistic-lock="version">
            <id name="id" type="int" column="id" />
            <property name="name" type="string" column="name" length="128" />
            <property name="url" type="string" column="url" length="516" />
    </class>
</hibernate-mapping>
