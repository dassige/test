<?xml version="1.0" encoding="utf-8"?>
<project name="antsimple" default="" basedir=".">
    <target name="createTmpDir" depends="deleteTmpDir">
        <mkdir dir="${env.JAVA_TMP_DIR}" />
    	<echo message="${env.JAVA_TMP_DIR} created"/>
    </target>
    <target name="deleteTmpDir">
        <delete dir="${env.JAVA_TMP_DIR} deleted" />
    </target>
</project>
